
===================================
== CONF1 export-tmpfs-B
===================================
headers:
  alias: alias
  name: name
  info: info
exports:
  - mode: tmpfs
    mount: /B

===================================
== CONF2 export-tmpfs-A
===================================
headers:
  alias: alias
  name: name
  info: info
exports:
  - mode: tmpfs
    mount: /A

===================================
redconf create --alias NODE --config export-tmpfs-B --config-adm export-tmpfs-A node/export-tmpfs-B/export-tmpfs-A
redwrap  --dump-only --redpath node/export-tmpfs-B/export-tmpfs-A
DUMP: /usr/bin/bwrap (as redwrap)
  --tmpfs /B
  --unshare-user
  --unshare-cgroup
  --unshare-ipc
  --unshare-pid
  --unshare-net
redwrap  --admin --dump-only --redpath node/export-tmpfs-B/export-tmpfs-A
DUMP: /usr/bin/bwrap (as redwrap)
  --tmpfs /B
  --tmpfs /A
  --unshare-user
  --unshare-cgroup
  --unshare-ipc
  --unshare-pid
  --unshare-net

===================================
redconf create --alias ROOT --config export-tmpfs-B --config-adm NULL rootnn/export-tmpfs-B
redconf create --alias SUBNODE --config export-tmpfs-A --config-adm NULL rootnn/export-tmpfs-B/export-tmpfs-A
redwrap  --dump-only --redpath rootnn/export-tmpfs-B/export-tmpfs-A
DUMP: /usr/bin/bwrap (as redwrap)
  --tmpfs /A
  --tmpfs /B
  --unshare-user
  --unshare-cgroup
  --unshare-ipc
  --unshare-pid
  --unshare-net
redwrap  --admin --dump-only --redpath rootnn/export-tmpfs-B/export-tmpfs-A
DUMP: /usr/bin/bwrap (as redwrap)
  --tmpfs /A
  --tmpfs /B
  --unshare-user
  --unshare-cgroup
  --unshare-ipc
  --unshare-pid
  --unshare-net

===================================
