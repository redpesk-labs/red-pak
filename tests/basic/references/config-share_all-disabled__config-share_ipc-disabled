
===================================
== CONF1 config-share_all-disabled
===================================
headers:
  alias: alias
  name: name
  info: info
config:
   share_all: disabled

===================================
== CONF2 config-share_ipc-disabled
===================================
headers:
  alias: alias
  name: name
  info: info
config:
   share_ipc: disabled

===================================
redconf create --alias NODE --config config-share_all-disabled --config-adm config-share_ipc-disabled node/config-share_all-disabled/config-share_ipc-disabled
redwrap  --dump-only --redpath node/config-share_all-disabled/config-share_ipc-disabled
DUMP: /usr/bin/bwrap (as redwrap)
  --unshare-user
  --unshare-cgroup
  --unshare-ipc
  --unshare-pid
  --unshare-net
redwrap  --admin --dump-only --redpath node/config-share_all-disabled/config-share_ipc-disabled
DUMP: /usr/bin/bwrap (as redwrap)
  --unshare-user
  --unshare-cgroup
  --unshare-ipc
  --unshare-pid
  --unshare-net

===================================
redconf create --alias ROOT --config config-share_all-disabled --config-adm NULL rootnn/config-share_all-disabled
redconf create --alias SUBNODE --config config-share_ipc-disabled --config-adm NULL rootnn/config-share_all-disabled/config-share_ipc-disabled
redwrap  --dump-only --redpath rootnn/config-share_all-disabled/config-share_ipc-disabled
DUMP: /usr/bin/bwrap (as redwrap)
  --unshare-user
  --unshare-cgroup
  --unshare-ipc
  --unshare-pid
  --unshare-net
redwrap  --admin --dump-only --redpath rootnn/config-share_all-disabled/config-share_ipc-disabled
DUMP: /usr/bin/bwrap (as redwrap)
  --unshare-user
  --unshare-cgroup
  --unshare-ipc
  --unshare-pid
  --unshare-net

