
===================================
== CONF1 config-share_ipc-enabled
===================================
headers:
  alias: alias
  name: name
  info: info
config:
   share_ipc: enabled

===================================
== CONF2 config-share_net-disabled
===================================
headers:
  alias: alias
  name: name
  info: info
config:
   share_net: disabled

===================================
redconf create --alias NODE --config config-share_ipc-enabled --config-adm config-share_net-disabled node/config-share_ipc-enabled/config-share_net-disabled
redwrap  --dump-only --redpath node/config-share_ipc-enabled/config-share_net-disabled
DUMP: /usr/bin/bwrap (as redwrap)
  --unshare-user
  --unshare-cgroup
  --unshare-pid
  --unshare-net
redwrap  --admin --dump-only --redpath node/config-share_ipc-enabled/config-share_net-disabled
DUMP: /usr/bin/bwrap (as redwrap)
  --unshare-user
  --unshare-cgroup
  --unshare-ipc
  --unshare-pid
  --unshare-net

===================================
redconf create --alias ROOT --config config-share_ipc-enabled --config-adm NULL rootnn/config-share_ipc-enabled
redconf create --alias SUBNODE --config config-share_net-disabled --config-adm NULL rootnn/config-share_ipc-enabled/config-share_net-disabled
redwrap  --dump-only --redpath rootnn/config-share_ipc-enabled/config-share_net-disabled
DUMP: /usr/bin/bwrap (as redwrap)
  --unshare-user
  --unshare-cgroup
  --unshare-ipc
  --unshare-pid
  --unshare-net
redwrap  --admin --dump-only --redpath rootnn/config-share_ipc-enabled/config-share_net-disabled
DUMP: /usr/bin/bwrap (as redwrap)
  --unshare-user
  --unshare-cgroup
  --unshare-ipc
  --unshare-pid
  --unshare-net

