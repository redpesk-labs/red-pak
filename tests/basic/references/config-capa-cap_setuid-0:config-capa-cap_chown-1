
===================================
== CONF1 config-capa-cap_setuid-0
===================================
headers:
  alias: alias
  name: name
  info: info
config:
   capabilities:
     - cap: cap_setuid
       add: 0

===================================
== CONF2 config-capa-cap_chown-1
===================================
headers:
  alias: alias
  name: name
  info: info
config:
   capabilities:
     - cap: cap_chown
       add: 1

===================================
redconf create --alias NODE --config config-capa-cap_setuid-0 --config-adm config-capa-cap_chown-1 node/config-capa-cap_setuid-0/config-capa-cap_chown-1
redwrap  --dump-only --redpath node/config-capa-cap_setuid-0/config-capa-cap_chown-1
DUMP: /usr/bin/bwrap (as redwrap)
  --cap-drop cap_setuid
  --unshare-user
  --unshare-cgroup
  --unshare-ipc
  --unshare-pid
  --unshare-net
redwrap  --admin --dump-only --redpath node/config-capa-cap_setuid-0/config-capa-cap_chown-1
DUMP: /usr/bin/bwrap (as redwrap)
  --cap-drop cap_setuid
  --unshare-user
  --unshare-cgroup
  --unshare-ipc
  --unshare-pid
  --unshare-net

===================================
redconf create --alias ROOT --config config-capa-cap_setuid-0 --config-adm NULL rootnn/config-capa-cap_setuid-0
redconf create --alias SUBNODE --config config-capa-cap_chown-1 --config-adm NULL rootnn/config-capa-cap_setuid-0/config-capa-cap_chown-1
redwrap  --dump-only --redpath rootnn/config-capa-cap_setuid-0/config-capa-cap_chown-1
DUMP: /usr/bin/bwrap (as redwrap)
  --cap-drop cap_setuid
  --cap-add cap_chown
  --unshare-user
  --unshare-cgroup
  --unshare-ipc
  --unshare-pid
  --unshare-net
redwrap  --admin --dump-only --redpath rootnn/config-capa-cap_setuid-0/config-capa-cap_chown-1
DUMP: /usr/bin/bwrap (as redwrap)
  --cap-drop cap_setuid
  --cap-add cap_chown
  --unshare-user
  --unshare-cgroup
  --unshare-ipc
  --unshare-pid
  --unshare-net

===================================
