
===================================
== CONF1 export-restrictedfile-A
===================================
headers:
  alias: alias
  name: name
  info: info
exports:
  - mode: restrictedfile
    mount: /A
    path: /somewhere

===================================
== CONF2 export-public-A
===================================
headers:
  alias: alias
  name: name
  info: info
exports:
  - mode: public
    mount: /A
    path: /somewhere

===================================
redconf create --alias NODE --config export-restrictedfile-A --config-adm export-public-A node/export-restrictedfile-A/export-public-A
redwrap  --dump-only --redpath node/export-restrictedfile-A/export-public-A
DUMP: /usr/bin/bwrap (as redwrap)
  --ro-bind /somewhere /A
  --unshare-user
  --unshare-cgroup
  --unshare-ipc
  --unshare-pid
  --unshare-net
redwrap  --admin --dump-only --redpath node/export-restrictedfile-A/export-public-A
DUMP: /usr/bin/bwrap (as redwrap)
  --bind /somewhere /A
  --unshare-user
  --unshare-cgroup
  --unshare-ipc
  --unshare-pid
  --unshare-net

===================================
redconf create --alias ROOT --config export-restrictedfile-A --config-adm NULL rootnn/export-restrictedfile-A
redconf create --alias SUBNODE --config export-public-A --config-adm NULL rootnn/export-restrictedfile-A/export-public-A
redwrap  --dump-only --redpath rootnn/export-restrictedfile-A/export-public-A
DUMP: /usr/bin/bwrap (as redwrap)
  --bind /somewhere /A
  --unshare-user
  --unshare-cgroup
  --unshare-ipc
  --unshare-pid
  --unshare-net
redwrap  --admin --dump-only --redpath rootnn/export-restrictedfile-A/export-public-A
DUMP: /usr/bin/bwrap (as redwrap)
  --bind /somewhere /A
  --unshare-user
  --unshare-cgroup
  --unshare-ipc
  --unshare-pid
  --unshare-net

===================================
redconf create --alias ROOT --config export-restrictedfile-A --config-adm NULL rootna/export-restrictedfile-A
redconf create --alias SUBNODE --config NULL --config-adm export-public-A rootna/export-restrictedfile-A/export-public-A
redwrap  --dump-only --redpath rootna/export-restrictedfile-A/export-public-A
DUMP: /usr/bin/bwrap (as redwrap)
  --ro-bind /somewhere /A
  --unshare-user
  --unshare-cgroup
  --unshare-ipc
  --unshare-pid
  --unshare-net
redwrap  --admin --dump-only --redpath rootna/export-restrictedfile-A/export-public-A
