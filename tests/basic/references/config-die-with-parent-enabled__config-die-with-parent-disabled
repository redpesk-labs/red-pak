
===================================
== CONF1 config-die-with-parent-enabled
===================================
headers:
  alias: alias
  name: name
  info: info
config:
   die-with-parent: enabled

===================================
== CONF2 config-die-with-parent-disabled
===================================
headers:
  alias: alias
  name: name
  info: info
config:
   die-with-parent: disabled

===================================
redconf create --alias NODE --config config-die-with-parent-enabled --config-adm config-die-with-parent-disabled node/config-die-with-parent-enabled/config-die-with-parent-disabled
redwrap  --dump-only --redpath node/config-die-with-parent-enabled/config-die-with-parent-disabled
DUMP: /usr/bin/bwrap (as redwrap)
  --die-with-parent
  --unshare-user
  --unshare-cgroup
  --unshare-ipc
  --unshare-pid
  --unshare-net
redwrap  --admin --dump-only --redpath node/config-die-with-parent-enabled/config-die-with-parent-disabled
DUMP: /usr/bin/bwrap (as redwrap)
  --unshare-user
  --unshare-cgroup
  --unshare-ipc
  --unshare-pid
  --unshare-net

===================================
redconf create --alias ROOT --config config-die-with-parent-enabled --config-adm NULL rootnn/config-die-with-parent-enabled
redconf create --alias SUBNODE --config config-die-with-parent-disabled --config-adm NULL rootnn/config-die-with-parent-enabled/config-die-with-parent-disabled
redwrap  --dump-only --redpath rootnn/config-die-with-parent-enabled/config-die-with-parent-disabled
DUMP: /usr/bin/bwrap (as redwrap)
  --unshare-user
  --unshare-cgroup
  --unshare-ipc
  --unshare-pid
  --unshare-net
redwrap  --admin --dump-only --redpath rootnn/config-die-with-parent-enabled/config-die-with-parent-disabled
DUMP: /usr/bin/bwrap (as redwrap)
  --unshare-user
  --unshare-cgroup
  --unshare-ipc
  --unshare-pid
  --unshare-net

