
===================================
== CONF1 export-private-A
===================================
headers:
  alias: alias
  name: name
  info: info
exports:
  - mode: private
    mount: /A
    path: /somewhere

===================================
== CONF2 export-publicfile-A
===================================
headers:
  alias: alias
  name: name
  info: info
exports:
  - mode: publicfile
    mount: /A
    path: /somewhere

===================================
redconf create --alias NODE --config export-private-A --config-adm export-publicfile-A node/export-private-A/export-publicfile-A
redwrap  --dump-only --redpath node/export-private-A/export-publicfile-A
DUMP: /usr/bin/bwrap (as redwrap)
  --bind /somewhere /A
  --unshare-user
  --unshare-cgroup
  --unshare-ipc
  --unshare-pid
  --unshare-net
redwrap  --admin --dump-only --redpath node/export-private-A/export-publicfile-A
DUMP: /usr/bin/bwrap (as redwrap)
  --bind /somewhere /A
  --bind /somewhere /A
  --unshare-user
  --unshare-cgroup
  --unshare-ipc
  --unshare-pid
  --unshare-net

===================================
redconf create --alias ROOT --config export-private-A --config-adm NULL rootnn/export-private-A
redconf create --alias SUBNODE --config export-publicfile-A --config-adm NULL rootnn/export-private-A/export-publicfile-A
redwrap  --dump-only --redpath rootnn/export-private-A/export-publicfile-A
DUMP: /usr/bin/bwrap (as redwrap)
  --bind /somewhere /A
  --unshare-user
  --unshare-cgroup
  --unshare-ipc
  --unshare-pid
  --unshare-net
redwrap  --admin --dump-only --redpath rootnn/export-private-A/export-publicfile-A
DUMP: /usr/bin/bwrap (as redwrap)
  --bind /somewhere /A
  --unshare-user
  --unshare-cgroup
  --unshare-ipc
  --unshare-pid
  --unshare-net

===================================
redconf create --alias ROOT --config export-private-A --config-adm NULL rootna/export-private-A
redconf create --alias SUBNODE --config NULL --config-adm export-publicfile-A rootna/export-private-A/export-publicfile-A
redwrap  --dump-only --redpath rootna/export-private-A/export-publicfile-A
DUMP: /usr/bin/bwrap (as redwrap)
  --unshare-user
  --unshare-cgroup
  --unshare-ipc
  --unshare-pid
  --unshare-net
redwrap  --admin --dump-only --redpath rootna/export-private-A/export-publicfile-A
DUMP: /usr/bin/bwrap (as redwrap)
  --bind /somewhere /A
  --unshare-user
  --unshare-cgroup
  --unshare-ipc
  --unshare-pid
  --unshare-net

