
===================================
== CONF1 export-internal-A
===================================
headers:
  alias: alias
  name: name
  info: info
exports:
  - mode: internal
    mount: /A
    path: /somewhere

===================================
== CONF2 export-restrictedfile-A
===================================
headers:
  alias: alias
  name: name
  info: info
exports:
  - mode: restrictedfile
    mount: /A
    path: /somewhere

===================================
redconf create --alias NODE --config export-internal-A --config-adm export-restrictedfile-A node/export-internal-A/export-restrictedfile-A
redwrap  --dump-only --redpath node/export-internal-A/export-restrictedfile-A
DUMP: /usr/bin/bwrap (as redwrap)
  --file /somewhere /A
  --unshare-user
  --unshare-cgroup
  --unshare-ipc
  --unshare-pid
  --unshare-net
redwrap  --admin --dump-only --redpath node/export-internal-A/export-restrictedfile-A
DUMP: /usr/bin/bwrap (as redwrap)
  --ro-bind /somewhere /A
  --unshare-user
  --unshare-cgroup
  --unshare-ipc
  --unshare-pid
  --unshare-net

===================================
redconf create --alias ROOT --config export-internal-A --config-adm NULL rootnn/export-internal-A
redconf create --alias SUBNODE --config export-restrictedfile-A --config-adm NULL rootnn/export-internal-A/export-restrictedfile-A
redwrap  --dump-only --redpath rootnn/export-internal-A/export-restrictedfile-A
DUMP: /usr/bin/bwrap (as redwrap)
  --ro-bind /somewhere /A
  --unshare-user
  --unshare-cgroup
  --unshare-ipc
  --unshare-pid
  --unshare-net
redwrap  --admin --dump-only --redpath rootnn/export-internal-A/export-restrictedfile-A
DUMP: /usr/bin/bwrap (as redwrap)
  --ro-bind /somewhere /A
  --unshare-user
  --unshare-cgroup
  --unshare-ipc
  --unshare-pid
  --unshare-net

===================================
redconf create --alias ROOT --config export-internal-A --config-adm NULL rootna/export-internal-A
redconf create --alias SUBNODE --config NULL --config-adm export-restrictedfile-A rootna/export-internal-A/export-restrictedfile-A
redwrap  --dump-only --redpath rootna/export-internal-A/export-restrictedfile-A
DUMP: /usr/bin/bwrap (as redwrap)
  --file /somewhere /A
  --unshare-user
  --unshare-cgroup
  --unshare-ipc
  --unshare-pid
  --unshare-net
redwrap  --admin --dump-only --redpath rootna/export-internal-A/export-restrictedfile-A
DUMP: /usr/bin/bwrap (as redwrap)
  --ro-bind /somewhere /A
  --unshare-user
  --unshare-cgroup
  --unshare-ipc
  --unshare-pid
  --unshare-net

===================================
