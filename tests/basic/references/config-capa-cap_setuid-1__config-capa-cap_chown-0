redwrap  --dump-only --redpath rootaa/config-capa-cap_setuid-1/config-capa-cap_chown-0
DUMP: /usr/bin/bwrap (as redwrap)
  --unshare-user
  --unshare-cgroup
  --unshare-ipc
  --unshare-pid
  --unshare-net
