
===================================
== CONF1 config-capa-cap_setuid-1
===================================
headers:
  alias: alias
  name: name
  info: info
config:
   capabilities:
     - cap: cap_setuid
       add: 1

===================================
== CONF2 config-capa-cap_chown-0
===================================
headers:
  alias: alias
  name: name
  info: info
config:
   capabilities:
     - cap: cap_chown
       add: 0

===================================
redconf create --alias NODE --config config-capa-cap_setuid-1 --config-adm config-capa-cap_chown-0 node/config-capa-cap_setuid-1/config-capa-cap_chown-0
redwrap  --dump-only --redpath node/config-capa-cap_setuid-1/config-capa-cap_chown-0
DUMP: /usr/bin/bwrap (as redwrap)
  --cap-add cap_setuid
  --unshare-user
  --unshare-cgroup
  --unshare-ipc
  --unshare-pid
  --unshare-net
redwrap  --admin --dump-only --redpath node/config-capa-cap_setuid-1/config-capa-cap_chown-0
DUMP: /usr/bin/bwrap (as redwrap)
  --cap-add cap_setuid
  --cap-drop cap_chown
  --unshare-user
  --unshare-cgroup
  --unshare-ipc
  --unshare-pid
  --unshare-net

===================================
redconf create --alias ROOT --config config-capa-cap_setuid-1 --config-adm NULL rootnn/config-capa-cap_setuid-1
redconf create --alias SUBNODE --config config-capa-cap_chown-0 --config-adm NULL rootnn/config-capa-cap_setuid-1/config-capa-cap_chown-0
redwrap  --dump-only --redpath rootnn/config-capa-cap_setuid-1/config-capa-cap_chown-0
DUMP: /usr/bin/bwrap (as redwrap)
  --cap-add cap_setuid
  --cap-drop cap_chown
  --unshare-user
  --unshare-cgroup
  --unshare-ipc
  --unshare-pid
  --unshare-net
redwrap  --admin --dump-only --redpath rootnn/config-capa-cap_setuid-1/config-capa-cap_chown-0
DUMP: /usr/bin/bwrap (as redwrap)
  --cap-add cap_setuid
  --cap-drop cap_chown
  --unshare-user
  --unshare-cgroup
  --unshare-ipc
  --unshare-pid
  --unshare-net

===================================
